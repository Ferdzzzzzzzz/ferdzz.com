{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "build": "npm run build:css && remix build",
    "dev": "concurrently \"npm run dev:css\" \"remix watch\"",
    "build:worker": "esbuild --define:process.env.NODE_ENV='\"production\"' --minify --bundle --sourcemap --outdir=dist ./worker",
    "dev:worker": "esbuild --define:process.env.NODE_ENV='\"development\"' --bundle --sourcemap --outdir=dist ./worker",
    "start": "miniflare --build-command \"npm run dev:worker\" --watch",
    "deploy": "npm run build && wrangler publish"
  },
  "dependencies": {
    "@hocuspocus/provider": "^1.0.0-alpha.29",
    "@radix-ui/react-alert-dialog": "^0.1.5",
    "@radix-ui/react-checkbox": "^0.1.4",
    "@radix-ui/react-context-menu": "^0.1.4",
    "@radix-ui/react-dropdown-menu": "^0.1.4",
    "@radix-ui/react-icons": "^1.0.3",
    "@radix-ui/react-radio-group": "^0.1.4",
    "@radix-ui/react-tooltip": "^0.1.6",
    "@remix-run/cloudflare-workers": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "@stitches/react": "^1.2.6",
    "@tldraw/core": "^1.5.1",
    "@tldraw/intersect": "^1.4.3",
    "@tldraw/vec": "^1.4.3",
    "idb-keyval": "^6.0.3",
    "perfect-cursors": "^1.0.5",
    "perfect-freehand": "^1.0.16",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.2.0",
    "react-hotkeys-hook": "^3.4.0",
    "remix": "^1.1.3",
    "tslib": "^2.3.1",
    "yjs": "^13.5.24",
    "zustand": "^3.6.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "esbuild": "0.13.14",
    "miniflare": "2.0.0-next.3",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
