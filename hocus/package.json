{
  "name": "hocus",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:worker": "tsc && esbuild --define:process.env.NODE_ENV='\"production\"' --minify --bundle --sourcemap --outdir=dist",
    "dev:worker": "esbuild --define:process.env.NODE_ENV='\"development\"' --bundle --sourcemap --outdir=dist",
    "start": "miniflare --build-command \"npm run dev:worker\" --watch",
    "deploy": "npm run build && wrangler publish",
    "compile": "tsc -w"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hocuspocus/server": "^1.0.0-alpha.91"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "esbuild": "^0.14.12",
    "miniflare": "^2.2.0",
    "typescript": "^4.5.5"
  }
}